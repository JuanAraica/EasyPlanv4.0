<select class="chosen" id="CedulaTra" name="CedulaTra" style="margin: 0 auto;">
    <option value="" disabled selected hidden>Select a worker</option>
    @foreach (var item in Model)
    {
        <option value="@Html.DisplayFor(modelItem => item.Nombre) @Html.DisplayFor(modelItem => item.Apellido)">@Html.DisplayFor(modelItem => item.Nombre) @Html.DisplayFor(modelItem => item.Apellido)</option>
    }

</select>
<script type="text/javascript">$(".chosen").chosen();</script>